+export function serializeToCsv(data: any[], hasHeader: boolean = true, delimiter: string = ","): string
+{
+    let csv = "";
+    if (hasHeader)
+        csv += data[0].join(delimiter) + "\n";
+    for (let i = 1; i < data.length; i++)
+        csv += data[i].join(delimiter) + "\n";
+    return csv;
+}
